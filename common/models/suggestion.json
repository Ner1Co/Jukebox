{
  "name": "Suggestion",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "date": {
      "type": "date"
    },
    "played": {
      "type": "boolean",
      "default": false
    },
    "playDate": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "votes":{
      "type": "hasMany",
      "model": "Vote"
    },
    "song": {
      "type": "belongsTo",
      "model": "Song"
    }
  },
  "acls": [],
  "methods": {}
}
